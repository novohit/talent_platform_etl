# 环境变量配置演示插件 - 环境配置示例
# 复制此文件为 .env 并根据需要修改配置

# =============================================================================
# 基础配置
# =============================================================================
PLUGIN_NAME=env_demo
DEBUG=true
LOG_LEVEL=DEBUG

# =============================================================================
# 业务配置
# =============================================================================
MAX_ITEMS=500
TIMEOUT=45.5
RETRY_COUNT=5
BATCH_SIZE=50

# =============================================================================
# 功能开关 (逗号分隔列表)
# =============================================================================
FEATURES_ENABLED=basic,advanced,premium
CACHE_ENABLED=true
MONITORING_ENABLED=true

# =============================================================================
# 外部服务配置
# =============================================================================
API_BASE_URL=https://api.env-demo.com
API_KEY=sk-1234567890abcdef
API_VERSION=v2

# =============================================================================
# 数据库配置
# =============================================================================
DB_HOST=db.mycompany.com
DB_PORT=3306
DB_NAME=env_demo_db
DB_USER=env_demo_user
DB_PASSWORD=secure_password_123

# =============================================================================
# 高级配置 (JSON格式)
# =============================================================================
CUSTOM_SETTINGS={"theme": "dark", "language": "zh-CN", "auto_save": true}
MAPPING_RULES={"user_id": "id", "user_name": "name", "user_email": "email"}

# =============================================================================
# 文件路径配置
# =============================================================================
DATA_DIR=/var/lib/plugin_data
LOG_DIR=/var/log/plugins
BACKUP_DIR=/var/backup/plugins

# =============================================================================
# 安全配置
# =============================================================================
SSL_VERIFY=true
ENCRYPTION_KEY=your_encryption_key_here

# env_demo 插件专用环境变量
# 这些配置会覆盖全局 plugins/.env 中的相同配置项

# 插件特定配置（覆盖全局配置）
LOG_LEVEL=DEBUG
BATCH_SIZE=50

# 插件独有配置
PLUGIN_VERSION=1.0.0
DEMO_MESSAGE=Hello from env_demo plugin!
DEMO_ENABLED=true
DEMO_MAX_ITERATIONS=10

# 特定的数据库配置（覆盖全局）
DB_NAME=env_demo_db
DB_USER=env_demo_user

# 插件专用的API密钥
DEMO_API_KEY=demo_secret_key_12345
DEMO_WEBHOOK_URL=https://webhook.env-demo.com/receive

# 临时文件配置
TEMP_PREFIX=env_demo_
CACHE_TTL=3600 