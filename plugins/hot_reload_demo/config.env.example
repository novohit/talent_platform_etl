# 热加载演示插件环境配置示例
# 复制此文件为 .env 并根据需要修改配置

PLUGIN_NAME=hot_reload_demo
PLUGIN_VERSION=1.0.0
DEBUG_MODE=true
MAX_RETRY_COUNT=3
DEFAULT_MESSAGE_PREFIX=[DEMO]
API_TIMEOUT=30
LOG_LEVEL=INFO

# 模拟的外部服务配置
EXTERNAL_API_URL=https://api.example.com
EXTERNAL_API_KEY=demo_key_12345
CACHE_TTL=300

# 插件特定配置
DEMO_ENABLED=true
DEMO_BATCH_SIZE=100 